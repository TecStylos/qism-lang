{
    "name": "qism-lang",
    "displayName": "QISM Lang",
    "description": "Syntax highlighting for QISM",
    "icon": "images/eQosys-logo-round.png",
    "version": "0.0.6",
    "engines": {
        "vscode": "^1.66.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/eQosys/qism-lang.git"
    },
    "publisher": "TecStylos",
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "qism",
            "aliases": ["QISM", "qism"],
            "extensions": [".qsm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "qism",
            "scopeName": "source.qsm",
            "path": "./syntaxes/qism.tmLanguage.json"
        }]
    }
}